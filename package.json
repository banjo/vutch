    "license": "MIT",
    "homepage": "https://github.com/banjo/wetch#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/banjo/wetch.git"
    },
    "bugs": "https://github.com/banjo/wetch/issues",
    "keywords": [],
    "sideEffects": false,
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": "dist/index.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint --ext .js,.ts,.json .",
        "prepublishOnly": "nr build",
        "release": "bumpp && npm publish",
        "start": "esno src/index.ts",
        "test": "vitest --watch=false",
        "test:dev": "vitest",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@clack/prompts": "^0.6.3",
        "chokidar": "^3.5.3",
        "cleye": "^1.3.2",
        "clipboardy": "^3.0.0",
        "execa": "^7.0.0",
        "globby": "^13.1.3",
        "picocolors": "^1.0.0",
        "string-argv": "^0.3.1"
    },
    "devDependencies": {
        "@antfu/ni": "^0.20.0",
        "@banjoanton/eslint-config": "^0.1.1",
        "@banjoanton/prettier-config": "^0.1.1",
        "@banjoanton/utils": "^0.1.38",
        "@types/minimist": "^1.2.2",
        "@types/node": "^18.14.6",
        "bumpp": "^9.0.0",
        "eslint": "^8.35.0",
        "esno": "^0.16.3",
        "lint-staged": "^13.1.2",
        "pnpm": "^7.29.0",
        "prettier": "^2.8.4",
        "simple-git-hooks": "^2.8.1",
        "tsup": "^6.6.3",
        "type-fest": "^3.6.1",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
        "vitest": "^0.29.2"
    },
    "prettier": "@banjoanton/prettier-config",
    "simple-git-hooks": {
        "pre-commit": "pnpm exec lint-staged"
    },
    "lint-staged": {
        "*": [
            "prettier --write --ignore-unknown",
            "eslint --fix"
        ]
    },
    "ling-staged": {
        "*": [
            "prettier --write --ignore-unknown",
            "eslint --fix"
        ]
    }
